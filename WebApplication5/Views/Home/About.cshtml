@model List<VideoResultViewModel>
@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-12" style="text-align:center">
        <h1 class="section-heading wow fadeInUp">视频下载器</h1>
        <p class="section-desc wow fadeInUp">下载Twitter视频 - 在线下载器</p>
    </div>
</div>

<div class="row">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.HrefAttribute</td>
                    <td>@item.Resolution</td>
                    <td>john@example.com</td>
                </tr>
            }

        </tbody>
    </table>
</div>
